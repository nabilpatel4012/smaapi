{
  "version": "1.0.0",
  "scripts": {
    "dev": "tsx --watch src/main.ts",
    "build": "tsc",
    "test": "vitest",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "db:seed": "tsx src/dev/seed.ts",
    "db:migrate-up": "tsx src/db/migrate.ts up",
    "db:migrate-down": "tsx src/db/migrate.ts down",
    "dev:docker": "docker compose -f docker-compose.yml -f docker-compose.dev.yml up --build -d",
    "dev:docker:down": "docker compose -f docker-compose.yml -f docker-compose.dev.yml down"
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.1",
    "@fastify/cors": "^10.0.2",
    "@fastify/jwt": "^9.0.3",
    "@fastify/swagger": "^9.4.0",
    "@fastify/swagger-ui": "^5.2.0",
    "argon2": "^0.41.1",
    "axios": "^1.7.9",
    "dotenv": "^16.4.7",
    "env-schema": "^6.0.0",
    "fastify": "^5.2.0",
    "fastify-plugin": "^5.0.1",
    "fastify-type-provider-zod": "^4.0.2",
    "http-status-codes": "^2.3.0",
    "ioredis": "^5.4.2",
    "kysely": "^0.27.5",
    "mongoose": "^8.10.0",
    "mysql2": "^3.12.0",
    "pg": "^8.13.1",
    "pg-error-enum": "^0.7.3",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0",
    "postgres": "^3.4.5",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "slugify": "^1.6.6",
    "zennv": "^0.1.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@faker-js/faker": "^9.3.0",
    "@fastify/secure-session": "^8.1.0",
    "@testcontainers/postgresql": "^10.16.0",
    "@types/node": "^22.10.1",
    "@types/pg": "^8.11.11",
    "eslint": "^9.17.0",
    "globals": "^15.14.0",
    "json-schema-to-ts": "^3.1.1",
    "testcontainers": "^10.16.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.1",
    "vitest": "^2.1.8"
  }
}